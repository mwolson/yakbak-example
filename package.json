{
  "name": "yakbak-example",
  "version": "0.0.1",
  "description": "Example yakbak setup with mocking",
  "engines": {
    "node": "4.2.6"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mwolson/yakbak-example.git"
  },
  "scripts": {
    "build": "npm run build:prod",
    "build:dev": "npm run build:dev:bundle",
    "build:dev:bundle": "grunt build-dev",
    "build:prod": "npm-run-all build:prod:backend build:prod:bundle",
    "build:prod:backend": "babel src/main/js -d dist",
    "build:prod:bundle": "grunt build-production",
    "clean": "rimraf dist",
    "link:this": "npm link",
    "lint": "grunt test",
    "mocha": "mocha",
    "start": "npm run start:prod",
    "start:dev": "npm-run-all build:dev start:dev:server",
    "start:dev:server": "babel-node src/main/js/backend/start.js",
    "start:mocking": "babel-node src/main/js/clients/mocking/start.js",
    "start:prod": "npm-run-all clean build:prod start:prod:server",
    "start:prod:server": "node dist/backend/start.js",
    "test": "npm-run-all --silent lint test:fast test:slow",
    "pretest:fast": "jsh echo \"> test:fast\"",
    "test:fast": "mocha src/test/js/fast-mocha",
    "pretest:slow": "jsh echo \"> test:slow\"",
    "test:slow": "mocha src/test/js/slow-mocha",
    "test:nightwatch": "npm-run-all selenium:install nightwatch",
    "version": "npm-run-all clean test build",
    "watch": "npm-run-all clean build:dev link:this watch:bundle",
    "watch:bundle": "grunt watch"
  },
  "author": "Michael Olson",
  "license": "BSD-2-Clause",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-plugin-module-alias": "^1.2.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.7.2",
    "babel-runtime": "^6.6.1",
    "bluebird": "^3.3.4",
    "eslint": "^2.8.0",
    "eslint-plugin-react": "^5.0.1",
    "express": "^4.13.4",
    "express-promise-router": "^1.0.0",
    "jsh-cli": "^0.1.4",
    "jsx-loader": "^0.12.2",
    "lodash": "^3.9.3",
    "message-format": "1.2.1",
    "mocha": "^2.4.5",
    "npm-run-all": "^1.7.0",
    "performance-now": "^0.2.0",
    "rimraf": "^2.5.2",
    "server-destroy": "^1.0.1",
    "superagent": "^1.8.3",
    "superagent-bluebird-promise": "^3.0.0",
    "supertest": "^1.2.0",
    "supertest-as-promised": "^3.1.0",
    "testdouble": "^1.2.0",
    "testdouble-chai": "^0.2.2",
    "uuid": "^2.0.2",
    "winston": "^2.2.0",
    "yakbak": "^2.4.3"
  }
}
